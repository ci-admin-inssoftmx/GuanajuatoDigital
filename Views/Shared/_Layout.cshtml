    @using System.Globalization

    <script type="text/javascript">
        //set culture of the Kendo UI
        kendo.culture("es-ES");
    </script>

    <script>
        $(document).ready(function () {
            var _Generales = [250];
            var _vehiculos = [230, 201, 260, 220, 280, 210, 240, 300, 310, 320, 270, 290];
            var _infracciones = [460, 401, 410, 420, 440, 490, 430, 480, 450];
            var _infraccionesInvisibles = [1600, 1610];
            var _accidentes = [630, 601, 610, 620];
            var _catalogos = [1140, 1160, 1170, 1001, 1210, 1190, 1090, 1220, 1020, 1120, 1040, 1200, 1240, 1230, 1180, 1060, 1110, 1030, 1050, 1130, 1100, 1070, 1150, 1080, 1010];
            var _blocs = [1400, 1410, 1420];
            var _configParametro = [1500,1520];
            var _firmas = [1300,1310, 1320];


            $.ajax({
                url: '@Url.Action("GetIdsPermitidos", "Login")',
                type: 'GET',
                success: function (data) {
                    var idsPermitidos = data;

                    // Mostrar solo los elementos que tienen un ID permitido o la clase 'always-visible'
                    $(".nav-item").each(function () {
                        var id = parseInt($(this).data("id"));
                        if (idsPermitidos.includes(id) || $(this).hasClass("always-visible")) {
                            $(this).prop('hidden', false);//.show();
                        }
                    });

                    let _vehiculoEncontrado = _vehiculos.some(id => idsPermitidos.includes(id));
                    let _infraccionEncontrada = _infracciones.some(id => idsPermitidos.includes(id));
                    let _infraccionInvisibleEncontrada = _infraccionesInvisibles.some(id => idsPermitidos.includes(id));;
                    let _accidenteEncontrado = _accidentes.some(id => idsPermitidos.includes(id));
                    let _catalogoEncontrado = _catalogos.some(id => idsPermitidos.includes(id));
                    let _GeneralesEncontrado = _Generales.some(id => idsPermitidos.includes(id));
                    let _BlocsEncontrado = _blocs.some(id => idsPermitidos.includes(id));
                    let _FirmasEncontrado = _firmas.some(id => idsPermitidos.includes(id));
                    let _DatosConfEncontrado = _configParametro.some(id => idsPermitidos.includes(id));

                    // Mostrar solo los tabs que correspondan a los IDs permitidos
                    if (_vehiculoEncontrado)
                        $("#nav-vehiculo-tab").prop('hidden', false);
                    if (_infraccionEncontrada)
                        $("#nav-infraccion-tab").prop('hidden', false);
                    if (_infraccionInvisibleEncontrada)
                        $("#nav-infraccion-invisible-tab").prop('hidden', false);
                    if (_accidenteEncontrado)
                        $("#nav-accidentes-tab").prop('hidden', false);
                    if (_catalogoEncontrado)
                        $("#nav-catalogo-tab").prop('hidden', false);
                    if (_GeneralesEncontrado)
                        $("#nav-General-tab").prop('hidden', false);
                    if (_BlocsEncontrado)
                        $("#nav-blocs-tab").prop('hidden', false);
                    if (_FirmasEncontrado)
                        $("#nav-firmas-tab").prop('hidden', false);
                    if (_DatosConfEncontrado)
                        $("#nav-config-param-tab").prop('hidden', false);

                    hideLoading();
                },
                error: function (error) {
                    console.log('Error al obtener los IDs permitidos.');
                    hideLoading();
                }
            });
        });
    </script>
